<%= simple_form_for(@fashion_jury, html: {class: "form-horizontal"}) do |f| %>

 <%= f.input :title %>

  <%= f.input :content %>

  <%= f.input :score %>

  <%= f.input :fashion_jury_image, as: :file %>
  <%= f.input :published_at, as: :string %>

  <div class="form-actions">
    <%= f.button :submit, class: "btn"%>
  </div>
<% end %>
